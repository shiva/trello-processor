{
  "name": "trelp",
  "version": "0.0.1",
  "description": "Search for listings in cards in trello JSON and output well-formatted text",
  "private": true,
  "bin": {
    "trelp": "./bin/trelp"
  },
  "scripts": {
    "test": "mocha",
    "coverage" : "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
