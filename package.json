    "commander": "~2.0.0",
    "autocmdr": "~0.0.4"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2"
  }
